    background-image:url('images/manualSelection.png');
}

.controlPanel .transcriptionIcon
{
    background-image:url('images/transcription.png');
}

.controlPanel .deleteIcon
{
    background-image:url('images/delete.png');
}

.controlPanel .mergeIcon
{
    background-image:url('images/merge.png');
}

.controlPanel .plus2Icon
{
    background-image:url('images/plus2.png');
}
.controlPanel .plus10Icon
{
    background-image:url('images/plus10.png');
}
.controlPanel .minus2Icon
{
    background-image:url('images/minus2.png');
}
.controlPanel .minus10Icon
{
    background-image:url('images/minus10.png');
}
.controlPanel .mergeIcon
{
    background-image:url('images/merge.png');
}

.controlPanel .seperator {
    height: 70%;
    width: 0.3%;
    float: left;
    background-color: #4B6AE7;
    border-radius: 3px;
    margin: 0.4%;
}

.controlPanel .statusLine
{
    float:right;
    padding:0.3%;
    font-size:small;
    width:25%;
}


.controlPanel .dropDown {
    float: left;
    height: 60%;
    padding: 0.6%;
    width: 12%;
}

.controlPanel .dropDown select {
    height: 100%;
    width: 100%;
    font-size: large;
    border: none;
    border-radius: 3px;
    background-color: #4B6AE7;
    box-shadow: 5px 5px 5px black;
}

.ui-resizable 
{
    position:absolute !important;
}