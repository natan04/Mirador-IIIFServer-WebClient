{
  "customConfig": {
              "annotationEndpoint": {
                    "name": "Simple Annotation Store Endpoint",
                    "module": "SimpleASEndpoint",
                    "options": {
                           "url": "${mirador.anno.url}",
                            "storeId": "comparison",
                            "APIKey": "user_auth"
                        }
              }
    },


  "services": [
    {
      "name": "PictureHandler",
      "baseUrl": "${mirador.picturehandler.base}"  ,
      "cmds": {
        "list": "Json?id=all",
        "upload": "Upload",
        "get": "Json?id="
      },
      "timeout": 2000
    },
  
    {
      "name": "InvokerService",
      "baseUrl": "${mirador.invoker.base}",
      "cmds" : {},
      "timeout": 2000,
      "batcherUrl": "${mirador.batcher.url}"
    }

  ]
}
